{
    "name": "commadore/graphql-client",
    "description": "A php GraphQL Client.",
    "type": "library",
    "keywords": [
        "GraphQL",
        "Client"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "commadore"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Commadore\\GraphQLClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Commadore\\GraphQLClient\\Test\\": "tests/"
        }
    }
}
